{
  "name": "vue-simple-dnd",
  "version": "0.5.0",
  "description": "A simple Drag and Drop library for Vue 3",
  "author": {
    "email": "contact@florian-lefebvre.dev",
    "name": "Florian Lefebvre",
    "url": "https://florian-lefebvre.dev"
  },
  "license": "MIT",
  "keywords": [
    "vue",
    "dnd",
    "components",
    "composables"
  ],
  "homepage": "https://github.com/florian-lefebvre/vue-simple-dnd",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/florian-lefebvre/vue-simple-dnd.git"
  },
  "bugs": {
    "url": "https://github.com/florian-lefebvre/vue-simple-dnd/issues"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup && vue-tsc"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "tsup": "^8.1.0",
    "typescript": "^5.2.2",
    "unplugin-vue": "^5.0.5",
    "vue-tsc": "^1.8.25"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "@vueuse/core": "^10.7.2",
    "@xstate/vue": "^3.0.3",
    "immer": "^10.0.3",
    "xstate": "^5.6.2"
  }
}
